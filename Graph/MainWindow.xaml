<Window x:Class="Graph.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Graph"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="canvas" Margin="0,0,0,151"/>
        <TextBlock x:Name="shortestPathTextBlock" Margin="0,325,0,10" FontSize="16" Panel.ZIndex="10"/>
        <TextBox x:Name="totalLabel" Text="Suma total" FontSize="20" Width="300" Height="50" Margin="0,384,700,0" Panel.ZIndex="500"/>
    </Grid>
</Window>
